{
    "name": "laravel/laravel",
    "type": "project",
    "description": "LoliArt Edge Standing",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "3.2.0",
        "fruitcake/laravel-cors": "2.0.4",
        "guzzlehttp/guzzle": "7.4.1",
        "jenssegers/agent": "2.6.4",
        "laravel/cashier": "13.6.1",
        "laravel/framework": "8.76.2",
        "laravel/horizon": "5.7.17",
        "laravel/sanctum": "2.13.0",
        "laravel/tinker": "2.6.3",
        "nwidart/laravel-modules": "8.2.0",
        "orangehill/iseed": "3.0.1",
        "pragmarx/health": "1.1.3",
        "renatomarinho/laravel-page-speed": "2.0.0",
        "spatie/laravel-permission": "5.4.1",
        "spatie/laravel-pjax": "2.1.2",
        "spatie/laravel-responsecache": "7.1.0",
        "tightenco/ziggy": "1.4.2",
        "torann/geoip": "3.0.3"
    },
    "require-dev": {
        "beyondcode/laravel-query-detector": "1.5.0",
        "facade/ignition": "2.17.2",
        "fakerphp/faker": "1.17.0",
        "laravel/sail": "1.12.12",
        "laravel/telescope": "4.6.9",
        "mockery/mockery": "1.4.4",
        "nunomaduro/collision": "5.10.0",
        "phpunit/phpunit": "9.5.10",
        "squizlabs/php_codesniffer": "3.6.2",
        "theanik/laravel-more-command": "1.2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/"
        },
        "files": [
            "app/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
